{
  "info": "Exp_behavior MultiLayerPerceptron",
  "base_config": "configs/experiments/Behavior/exp_behavior_base.json",

   "observation": {
        "type": "MultiAgentObservation",

        "observation_config": {
            "type": "Kinematics"
        },
        "vehicles_count": 5,
        "features": ["presence", "x", "y", "vx", "vy", "is_controlled"],
        "action_history_type": "binary",
        "action_history_count": 10,
        "absolute": false,
        "order": "sorted_by_x",
        "see_behind": false,
        "normalize": true,
        "mission_vehicle_observation": true,
        "cooperative_perception":true
    },

    "agent_config": {
    "model": {
        "type": "MultiLayerPerceptron",
        "layers": [256, 256]
    }

  }


}