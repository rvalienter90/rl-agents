{
  "info": "NN exp1300",
  "additional_folder_name": "exp_merge_400",
  "base_config": "configs/experiments/complex/exp_merge_complex_400base-2.json",
  "agent_config": {
    "model": {
      "type": "EgoAttentionNetwork",
      "embedding_layer": {
        "type": "MultiLayerPerceptron",
        "layers": [
          64,
          64
        ],
        "reshape": false,
        "in": 7
      },
      "others_embedding_layer": {
        "type": "MultiLayerPerceptron",
        "layers": [
          64,
          64
        ],
        "reshape": false,
        "in": 7
      },
      "self_attention_layer": null,
      "attention_layer": {
        "type": "EgoAttention",
        "feature_size": 64,
        "heads": 2
      },
      "output_layer": {
        "type": "MultiLayerPerceptron",
        "layers": [
          64,
          64
        ],
        "reshape": false
      }
    },
    "gamma": 0.99,
    "batch_size": 64,
    "memory_capacity": 15000,
    "target_update": 512
  },

  "observation": {
    "type": "MultiAgentObservation",
    "observation_config": {
      "type": "Kinematics"
    },
    "vehicles_count": 10,
    "features": [
      "presence",
      "x",
      "y",
      "vx",
      "vy",
      "cos_h",
      "sin_h"
    ],
    "action_history_type": "discrete",
    "action_history_count": 10,
    "absolute": false,
    "order": "sorted_by_x",
    "see_behind": false,
    "normalize": true,
    "mission_vehicle_observation": true,
    "cooperative_perception": true
  }
}